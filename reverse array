package reverse;

public class array {
	public static void main(String args[]) {
		int [] a = new int[100];
		int n = a.length;
		for (int k =0; k<n; k++) 
				a[k]=k;
		for (int i = 0;i<n/2;i++)
		{
			int temp = a[i];
			a[i] = a[n-1-i];
			a[n-1-i] = temp;
			
		}
		for (int j = 0; j<n;j++)
			System.out.println(a[j]);
		
	}

}
